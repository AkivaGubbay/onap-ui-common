{
  "name": "onap-ui",
  "version": "4.0.0-alpha.9",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "node utils/delete-folders.js",
    "build": "npm run build-common",
    "build-common": "npm run prebuild && npm run build-icons && npm run copy-scss",
    "build-icons": "node utils/create-svg-icons-map.js",
    "copy-scss": "node utils/copy-scss-files.js",

    "prebuild2": "node utils/delete-folders.js",
    "build2": "npm run build-common",
    "build-common2": "npm run prebuild && npm run build-icons && npm run build-scss && npm run build-webpack && npm run build-rollup",
    "build-scss": "node-sass --include-path scss styles/style.scss .generated/style.css",
    "build-rollup": "rollup -c rollup.umd.config.js",
    "build-rollup1": "rollup utils/index.js --format umd --name 'onap-ui' --file lib/onap-ui.js",
    "build-webpack": "webpack"
  },
  "dependencies": {
    "delete": "^1.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "copy": "^0.3.2",
    "copy-dir": "^0.3.0",
    "css-loader": "^0.28.11",
    "exports-loader": "^0.7.0",
    "json-loader": "^0.5.7",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8"
  }
